			<form action="{{ path("app_dashboard_settings_email") }}" method="POST" class="mt-2 w-100">
				{% for message in app.flashes("email_update_error") %}
					<div class="alert alert-danger alert-dismissible fade show" role="alert">
						{{ message }}
						<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
					</div>
				{% endfor %}
				{% for message in app.flashes("email_update_success") %}
					<div class="alert alert-success alert-dismissible fade show" role="alert">
						{{ message }}
						<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
					</div>
				{% endfor %}
				<div class="form-floating mb-3 pb-1">
					<input type="email" class="form-control" id="email" name="email" placeholder="name@example.com" value="{{ email }}" required>
					<label for="email">E-mail address</label>
				</div>

				<input type="hidden" name="token" value="{{ csrf_token("updateEmail") }}">
				<a class="btn btn-primary btn-lg py-2 w-100" data-bs-toggle="modal" data-bs-target="#updateEmailModal">Update my e-mail</a>

				<div class="modal fade" tabindex="-1" id="updateEmailModal">
					<div class="modal-dialog" role="document">
						<div class="modal-content rounded-3 shadow">
							<div class="modal-body p-4 text-center">
								<h5 class="mb-0" id="updateEmailModalLabel">Are you sure you want to update your e-mail address?</h5>
								<p class="mb-0">You will be disconnected and you must click on a validation link that you will receive in an e-mail to your new e-mail address.</p>
							</div>
							<div class="modal-footer flex-nowrap p-0">
								<input class="btn btn-lg btn-secondary fs-6 text-decoration-none col-6 py-3 m-0 rounded-0 fw-bold border-end" type="submit" value="Update my e-mail" data-bs-dismiss="modal"></input>
							<button type="button" class="btn btn-lg fs-6 text-decoration-none col-6 py-3 m-0 rounded-0" data-bs-dismiss="modal">Cancel</button>
						</div>
					</div>
				</div>
			</div>
		</form>
	</div>
